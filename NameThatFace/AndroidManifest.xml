<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.final_proj.winslow"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/question_face"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.final_proj.winslow.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name = "com.final_proj.winslow.Game"/>
        <activity android:name = "com.final_proj.winslow.Finished"/>
        <activity android:name = "com.final_proj.winslow.HowToPlay"/>
    </application>

</manifest>